{
  "name": "buddy-bot/demo",
  "description": "Demo PHP project for testing Buddy Bot Composer support",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Buddy Bot",
      "email": "hello@buddy-bot.dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^v10.0.3",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/console": "^v6.0.19",
    "monolog/monolog": "^3.0",
    "doctrine/dbal": "^3.10.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3.7",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.20",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^v7.0.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "pint": "pint",
    "pint-test": "pint --test"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
